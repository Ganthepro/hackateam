@{
    ViewData["Title"] = "Edit Profile";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/authen.css" asp-append-version="true" />
    <title>@ViewData["Title"] - EditProfile</title>
</head>
<body>  
    <section>
        <h1>Edit Profile</h1>
        <form id="editprofile">
            <label for="username">Username</label>
            <input type="text" id="username" name="username" />

            <label for="education">Education</label>
            <input type="text" id="education" name="education" />

            <label for="header">Education</label>
            <input type="text" id="header" name="header" />

            <label for="tel">Tel</label>
            <input type="tel" id="tel" name="tel" />

            <label for="email">Email</label>
            <input type="email" id="email" name="email" />

            <div class="submit"><button type="submit">Edit</button></div>
        </form>
    </section>
    <footer>
        <a asp-controller="Profile" asp-action="Index">Go To Profile</a>
        <a asp-controller="Profile" asp-action="EditProject">Add New Project</a>
    </footer>
</body>
</html>

